CREATE TABLE personal(
dni_personal integer NOT NULL,
personal_nombre varying(30),
personal_apellido varyng(30),
condicion_laboral varying(15),
cargo_p varying(35),
telefono integer
);
CREATE TABLE administrativo(
dni_admin integer NOT NULL,
admin_nombre varying(20),
admin_apellido varyin(20),
cargo_a varying(35),
),
CREATE TABLE medico(
dni_medico integer NOT NULL,
medico_nombre varying(20),
medico_apellido varyin(20),
cargo_m varying(35),
);
CREATE TABLE departamento(
cod_dep varying(6) NOT NULL,
dni_medico integer NOT NULL,
depart_nombre varying(50),
);
CREATE TABLE investiga{
id_inves integer,
dni_med integer,
}
CREATE TABLE investigacion(
id_inves integer NOT NULL,
nombre_inves char(130),
fecha_revision char(15),
tipo_estudio varying(15),
registro varying(15),
nombre_dep varying(50)
);
CREATE TABLE paciente(
dni_paciente integer NOT NULL,
paciente_nombre varying(20),
paciente_apellido varying(15),
fecha_nac varying(15),
telefono integer,
);
CREATE TABLE cita(
dni_paciente integer,
dni_med integer,
num_consultorio integer,
fecha_cita varying(15)
);
CREATE TABLE consultorio(
num_consultorio integer NOT NULL,
piso_consultorio integer,
especialidad varying(20)
);
CREATE TABLE curso(
id_curso varying(5),
dni_medico integer,
curso_nombre varying(40),
curso_departamento varying(50)
);
CREATE TABLE recibe{
id_curso varying(5),
dni_residente integer
}
CREATE TABLE residente(
dni_residente integer NOT NULL,
nombre_residente varying(12),
apellido_residente varying(12),
año_academico varying(12)
);
CREATE TABLE direccion{
cod_dep varying(6) NOT NULL,
nombre_dic varying(30)
}
CREATE TABLE articulos{
id_articulo integer,
cod_dep varying(6),
nombre_articulo varying(100),
revista varying(100),
tipo_cancer varying(12)
}
